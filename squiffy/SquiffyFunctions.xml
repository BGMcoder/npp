<NotepadPlus>
	<functionList>
		<!-- File format used for: .sq         -->
			<parser
				id="Squiffy" displayName="Squiffy" commentExpr=""
			>
				<function
					mainExpr="^\[(?:\[([^\[\]\r\n]+)\]|(?1))\]:"
				>
					<functionName>
						<!-- <nameExpr expr="^\[(?:\[([^\[\]\r\n]+)\]\](?=:)|\K(?1)(?=\]:))" /> -->
						<nameExpr expr="^\[(?:\[([^\[\]\r\n]+)(?=\]\]:)|\K(?1)(?=\]:))" />

					</functionName>
				</function>
			</parser>
	</functionList>
</NotepadPlus>
